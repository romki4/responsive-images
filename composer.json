{
    "name": "romki4/responsive-images",
    "description": "A Laravel package for generating responsive images and picture tags",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Romki4\\ResponsiveImages\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Romki4\\ResponsiveImages\\Providers\\ResponsiveImagesServiceProvider"
            ],
            "aliases": {
                "ResponsiveImages": "Romki4\\ResponsiveImages\\Facades\\ResponsiveImagesFacade"
            }
        }
    },
    "authors": [
        {
            "name": "Roman Mozghovyi"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "intervention/image": "^2.7"
    }
}
